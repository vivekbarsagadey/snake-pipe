# Environment variables for snake-pipe
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/snake_pipe
DB_HOST=localhost
DB_PORT=5432
DB_NAME=snake_pipe
DB_USER=postgres
DB_PASSWORD=your_password

# API Configuration
API_BASE_URL=https://api.example.com
API_KEY=your_api_key_here
API_TIMEOUT=30

# File Paths
DATA_DIR=./data
LOG_DIR=./logs

# Logging
LOG_LEVEL=INFO

# Pipeline Settings
BATCH_SIZE=1000
MAX_RETRIES=3

# Warehouse Settings (Optional)
# Snowflake
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_user
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema

# BigQuery
GOOGLE_CLOUD_PROJECT=your_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Redshift
REDSHIFT_HOST=your_cluster.region.redshift.amazonaws.com
REDSHIFT_PORT=5439
REDSHIFT_DB=your_database
REDSHIFT_USER=your_user
REDSHIFT_PASSWORD=your_password
